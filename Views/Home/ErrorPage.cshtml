@model int


@{ 
    ViewBag.Title = "404 Error";
}


<div class="alart alart-danger mt-1 mb-1">
    <h4>404 Not Found Error:</h4>
    <hr />
    <h5>Person with Id  = @Model Cannot be Found</h5>
</div>
<a asp-action="details" asp-controller="home" class="btn btn-outline-success" width: auto>
    Click here to see the list of all Persons
</a>

